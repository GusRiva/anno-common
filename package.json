{
  "private": true,
  "name": "anno",
  "scripts": {
    "bootstrap": "./node_modules/.bin/lerna bootstrap",
    "postinstall": "npm run bootstrap ; npm run link-all",
    "link-all": "for i in anno-*;do sh -c \"cd $i; npm link\";done",
    "docker": "docker build -t kbai/anno -f Dockerfile ."
  },
  "cacheDirectories": [
